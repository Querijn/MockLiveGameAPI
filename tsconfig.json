{
	"compilerOptions": {
		"noImplicitAny": true,
		"strictNullChecks": true,
		"alwaysStrict": true,
		"outDir": "dist",
		"module": "commonjs",
		"target": "es2017",
		"esModuleInterop": true,
		"sourceMap": true
	},
	"include": [
		"src/**/*"
	],
	"exclude": [
		"node_modules"
	],
	"typeRoots": [
		"/types"
	]
}